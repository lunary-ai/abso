{
  "name": "abso-ai",
  "version": "0.0.1",
  "module": "src/index.ts",
  "type": "module",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/lunary-ai/abso.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.3.6"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@google/generative-ai": "^0.21.0",
    "lunary": "^0.8.8",
    "openai": "^4.83.0"
  }
}
